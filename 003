pip install coreapi

import coreapi
import coreschema
from rest_framework import schemas


class XXXViewset(viewsets.ViewSet):
    schema = schemas.ManualSchema(fields=[
            coreapi.Field(
                'XXX_id',
                required=True,
                location="path",
                schema=coreschema.Array(
                    description="XXX_id"
                )
            ),
            coreapi.Field(
                'month',
                required=True,
                location="path",
                schema=coreschema.Array(
                    description="month2"
                )
            ),
        ])

    @action(detail=False, methods=['get'])
    def get_trend_words(self, request, collection_id, month):
