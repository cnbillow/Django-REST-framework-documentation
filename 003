urlpatterns = [
    url(r'^', include(router.urls)),
    url("trendwords/get_trend_words/(?P<collection_id>[a-zA-Z0-9]+)/(?P<month>[0-9]{6})/", TrendwordsViewset.as_view({'get': 'get_trend_words'}), name="get_trend_words"),
]

class TrendwordsViewset(viewsets.ViewSet):
    @action(detail=False, methods=['get'])
    def get_trend_words(self, request, collection_id, month):

